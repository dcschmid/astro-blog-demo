{
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-css-order",
    "prettier-plugin-astro-organize-imports"
  ],
  "cssDeclarationSorterOrder": "concentric-css",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
